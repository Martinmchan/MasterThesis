function outF = ourFuncCombo(param, tdoa12, tdoa13, tdoa14, micMatrix, combo)
x = param(1);
y = param(2);
z = param(3);

func1 = tdoa12 - sqrt((x-micMatrix(combo(1),1)).^2 + (y-micMatrix(1,2)).^2 + (z-micMatrix(1,3)).^2) + sqrt((x-micMatrix(2,1)).^2 + (y-micMatrix(2,2)).^2 + (z-micMatrix(2,3)).^2);
func2 = tdoa13 - sqrt((x-micMatrix(combo(1),1)).^2 + (y-micMatrix(1,2)).^2 + (z-micMatrix(1,3)).^2) + sqrt((x-micMatrix(3,1)).^2 + (y-micMatrix(3,2)).^2 + (z-micMatrix(3,3)).^2);
func3 = tdoa14 - sqrt((x-micMatrix(combo(1),1)).^2 + (y-micMatrix(1,2)).^2 + (z-micMatrix(1,3)).^2) + sqrt((x-micMatrix(4,1)).^2 + (y-micMatrix(4,2)).^2 + (z-micMatrix(4,3)).^2);

outF = [func1; func2; func3];

end