clear all
[mic1, Fs1] = audioread('cap116.wav');
[mic2, Fs2] = audioread('cap38.wav');

y1 = mic1(1:200000);
y2 = mic2(1:200000);

tdoa1 = ourGccphat(y1,y2)

y1 = mic1(200000:400000);
y2 = mic2(200000:400000);

tdoa2 = ourGccphat(y1,y2);

tdoa = (tdoa1 - tdoa2)/2;

tdoa = tdoa/Fs1;

deltaS = 343*tdoa