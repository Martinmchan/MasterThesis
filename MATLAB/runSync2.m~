close all;
clear all;

[mic1, f] = audioread('cap168.wav');
[mic2, f] = audioread('cap99.wav');
[mic3, f] = audioread('cap38.wav');

targetF = 4000;
N = 50;
freq_index = round(targetF/f*N) + 1;

for i = 1:N:700000
    dft_data = abs(goertzel(mic1(i:i+N),freq_index))
    plot(i,dft_data,'o')
    hold on
end
figure
 for i = 1:N:700000
    dft_data = abs(goertzel(mic2(i:i+N),freq_index))
    plot(i,dft_data,'o')
    hold on
end   
figure
for i = 1:N:700000
    dft_data = abs(goertzel(mic1(i:i+N),freq_index))
    plot(i,dft_data,'o')
    hold on
end

